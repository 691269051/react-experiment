    "less-loader": "7.3.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "react-app-rewired": "2.1.8",
    "react-scripts": "4.0.3",
    "sas": "3.0.4",
    "serve": "^12.0.0",
    "vscode-web-custom-data": "^0.3.4",
    "webpack-bundle-analyzer": "^4.4.2",
    "yarn-deduplicate": "^3.1.0"
  },
  "sideEffects": false,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,less,md}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
