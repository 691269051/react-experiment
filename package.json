{
  "name": "rcnext",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "proxy": "http://localhost:3000/",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@rematch/core": "^1.3.0",
    "@rematch/immer": "^1.2.0",
    "antd": "^3.26.4",
    "anujs": "^1.6.0",
    "classname": "^0.0.0",
    "date-fns": "^2.8.1",
    "immer": "^5.1.0",
    "less": "3.10.3",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "react": "^0.0.0-experimental-f42431abe",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dnd": "^10.0.2",
    "react-dom": "^0.0.0-experimental-f42431abe",
    "react-loadable": "^5.5.0",
    "react-motion": "^0.5.2",
    "react-redux": "^7.1.3",
    "react-transition-group": "^4.3.0",
    "react-use": "^13.12.2",
    "styled-components": "^4.4.1",
    "swr": "^0.1.15",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "analyz": "react-app-rewired build --analyze",
    "format": "./node_modules/.bin/prettier --single-quote --write src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md}",
    "update": "yarn upgrade-interactive --latest && yarn yarn-deduplicate yarn.lock",
    "start": "react-app-rewired start",
    "server": "node ./server",
    "build": "react-app-rewired build",
    "build:dev": "dotenv -e .env.development react-app-rewired build",
    "test": "react-app-rewired test",
    "clean": "rm -rf ./node_modules && rm -rf ./yarn.lock"
  },
  "devDependencies": {
    "@hardfist/rematch": "^1.3.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.2",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.0",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "babel-plugin-import": "^1.13.0",
    "colors": "^1.4.0",
    "cross-env": "^6.0.3",
    "customize-cra": "^0.9.1",
    "dotenv-cli": "^3.1.0",
    "express": "^4.17.1",
    "husky": "^3.1.0",
    "less-loader": "^5.0.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-app-rewired": "2.1.5",
    "react-hot-loader": "^4.12.18",
    "react-scripts": "3.3.0",
    "sas": "3.0.4",
    "webpack-bundle-analyzer": "^3.6.0",
    "yarn-deduplicate": "^1.1.1"
  },
  "sideEffects": false,
  "eslintConfig": {
    "extends": [
      "react-app",
      "./.eslintrc.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,less,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
