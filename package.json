{
  "name": "rcnext",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "proxy": "http://localhost:3000/",
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@rematch/core": "^1.4.0",
    "@rematch/immer": "^1.2.0",
    "antd": "^4.5.4",
    "anujs": "^1.6.2",
    "braft-editor": "^2.3.9",
    "classname": "^0.0.0",
    "date-fns": "^2.14.0",
    "immer": "^7.0.7",
    "less": "3.12.2",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "react": "^0.0.0-experimental-f77c7b9d7",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dnd": "^11.1.3",
    "react-dom": "^0.0.0-experimental-f77c7b9d7",
    "react-loadable": "^5.5.0",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^6.0.0-beta.0",
    "react-transition-group": "^4.4.1",
    "react-use": "^15.1.1",
    "slate": "^0.58.4",
    "slate-history": "^0.58.4",
    "slate-react": "^0.58.4",
    "styled-components": "^5.1.1",
    "swr": "^0.3.1",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "analyz": "react-app-rewired build --analyze",
    "format": "yarn prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md}",
    "update": "yarn upgrade-interactive --latest && yarn yarn-deduplicate yarn.lock",
    "start": "react-app-rewired start",
    "server": "node ./server",
    "server:build": "yarn server -s build",
    "build": "react-app-rewired build",
    "build:dev": "dotenv -e .env.development react-app-rewired build",
    "test": "react-app-rewired test",
    "clean": "rm -rf ./node_modules && rm -rf ./yarn.lock",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "crlf2lf": "find . \\( -path ./node_modules -o -path ./.git \\) -prune -o -type f -exec dos2unix -k {} +"
  },
  "devDependencies": {
    "@hardfist/rematch": "^1.3.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.1",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-links": "^6.0.16",
    "@storybook/addons": "^6.0.16",
    "@storybook/preset-create-react-app": "^3.0.0",
    "@storybook/react": "^6.0.16",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "^5.1.2",
    "babel-plugin-import": "^1.13.0",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^1.0.0",
    "dotenv-cli": "^3.1.0",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "husky": "^4.2.5",
    "less-loader": "^6.1.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "react-app-rewired": "2.1.6",
    "react-refresh": "^0.8.3",
    "react-scripts": "4.0.0",
    "sas": "3.0.4",
    "serve": "^11.3.1",
    "vscode-web-custom-data": "^0.3.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "yarn-deduplicate": "^2.0.0"
  },
  "sideEffects": false,
  "eslintConfig": {
    "extends": [
      "react-app",
      "./.eslintrc.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,less,md}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
