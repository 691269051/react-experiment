{
  "name": "rcnext",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "proxy": "http://localhost:3000/",
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@rematch/core": "^1.4.0",
    "@rematch/immer": "^1.2.0",
    "antd": "^4.9.2",
    "anujs": "^1.7.4",
    "braft-editor": "^2.3.9",
    "classname": "^0.0.0",
    "date-fns": "^2.16.1",
    "dayjs": "^1.9.7",
    "immer": "^8.0.0",
    "less": "3.12.2",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dnd": "^11.1.3",
    "react-dom": "^17.0.1",
    "react-loadable": "^5.5.0",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.0.0-beta.0",
    "react-transition-group": "^4.4.1",
    "react-use": "^15.3.4",
    "slate": "^0.59.0",
    "slate-history": "^0.59.0",
    "slate-react": "^0.59.0",
    "styled-components": "^5.2.1",
    "swr": "^0.3.9",
    "typescript": "4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "analyz": "react-app-rewired build --analyze",
    "format": "yarn prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md}",
    "update": "yarn upgrade-interactive --latest && yarn yarn-deduplicate yarn.lock",
    "start": "react-app-rewired start",
    "server": "node ./server",
    "server:build": "yarn server -s build",
    "build": "react-app-rewired build",
    "build:dev": "dotenv -e .env.development react-app-rewired build",
    "test": "react-app-rewired test",
    "clean": "rm -rf ./node_modules && rm -rf ./yarn.lock",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "crlf2lf": "find . \\( -path ./node_modules -o -path ./.git \\) -prune -o -type f -exec dos2unix -k {} +"
  },
  "devDependencies": {
    "@hardfist/rematch": "^1.3.0",
    "@storybook/addon-actions": "^6.1.0",
    "@storybook/addon-links": "^6.1.0",
    "@storybook/addons": "^6.1.0",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.1",
    "@testing-library/user-event": "^12.2.2",
    "@types/jest": "^26.0.17",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.11",
    "@types/styled-components": "^5.1.4",
    "babel-plugin-import": "^1.13.1",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^1.0.0",
    "dotenv-cli": "^4.0.0",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "husky": "^4.3.5",
    "less-loader": "^7.1.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.1.2",
    "react-app-rewired": "2.1.7",
    "react-scripts": "4.0.1",
    "sas": "3.0.4",
    "serve": "^11.3.1",
    "vscode-web-custom-data": "^0.3.1",
    "webpack-bundle-analyzer": "^4.1.0",
    "yarn-deduplicate": "^3.1.0"
  },
  "sideEffects": false,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,less,md}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
